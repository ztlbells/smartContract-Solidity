pragma solidty ^0.4.2;

import "truffle/Assert.sol";
import "truffle/DeployeAddresses.sol";
import "../contract/SimpleStorage.sol";

contract TestSimpleStorage {
    function testItStorageAValue(){
        SimpleStorage simpleStorage = SimpleStorage(DeployeAddresses.SimpleStorage());
        simpleStorage.set(89);
        Assert.equals(simpleStorage.get(),89,"It should store the value 89.");
    }
}